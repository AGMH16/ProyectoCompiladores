terminal Float DECIMAL;
terminal String IDENTIFICADOR,CADTEXTO;
terminal ERRORCERO,ERRORNUM,ERRORDECIM,ERRORCOM,ERROROPINC,ERROREAL,ERRORARIT,
         ERRORSIGP,ERROROPARBO,ERRORSIG,ERRORID,ERRORNAMECLASS,ERRORSIGNOS,
         ERRORSIGNOS1,ERRORSIGNOS2;
terminal TEXTO;
non terminal ID, IDCAD, IDINT, IDREAL, IDBOOL, DECVARIABLES, ARREGLOS, DECIMALES , ENTEROS, CADENAS, BOLEANOSTEXT,BOLEANOSNUM;
non terminal S,OPERACION,NUMEROS;
non terminal LECTURA;
non terminal ESCRITURA , CONTENIDOESCRI;
non terminal SUMOCOM,ASIGNAR ;
precedence right SUMA, RESTA, MULTIPLICACION, DIVISION, MOD, EXPONENTE,NUMERO,DECIMAL;
