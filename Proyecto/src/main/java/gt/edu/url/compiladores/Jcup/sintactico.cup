terminal METODO, PROPIEDAD, TDISPONIBLEM,TDISPONIBLEP;
terminal CLASE, NAMECLASS;
terminal INSTANCIAR, ELIMINAR;
terminal CADENAaENTERO,FUNespREAL,CADaBOOL;
terminal INCLUIR,EXTENCIONBIB;
terminal ESTATICO;
