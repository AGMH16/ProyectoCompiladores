/*Arreglos*/
DECIMALES ::= DECIMAL|
             DECIMAL COMA DECIMALES;
ENTEROS ::= ENTERO|
	   ENTERO COMA ENTEROS;
CADENAS ::= CADTEXTO|
	   CADTEXTO COMA CADENAS;

BOLEANOSTEXT ::= BOOLTEXT|
		BOOLTEXT COMA BOLEANOSTEXT;

BOLEANOSNUM ::= BOOLNUM|
	       BOOLNUM COMA BOLEANOSNUM ;
