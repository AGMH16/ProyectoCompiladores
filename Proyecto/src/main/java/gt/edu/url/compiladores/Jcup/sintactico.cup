terminal FINLINEA;
terminal COMILLAS,DOBLEIGUAL, NEGAR, PUNTO, SIGUAL, SUMA, RESTA, MULTIPLICACION,
         DIVISION, MOD,DOSPUNTOS, OPRELACIONAL, OPATRIBUCION, OPINCREMENTO, PARENTESISA, 
         PARENTESISC,LLAVEA, LLAVEC, CORCHETEA, CORCHETEC, BOOLTEXT, BOOLNUM, OPBOLEANO,
         PUNTOYCOMA, COMA, EXPONENTE, OPARBOOL;
terminal REAL,CADENA,NULO,ENTERO,BOLEANO;
terminal SI, ENTONCES,SINO,FINSI;
