/*------------------------------------------------------------------------------------------------------------------------------------------------*/            
DECVARIABLES ::= REAL IDREAL| 
                 CADENA IDCAD |
                 ENTERO IDINT|
                 BOLEANO IDBOOL| 
                 REAL IDREAL FINLINEA| 
                 CADENA IDCAD FINLINEA|
                 ENTERO IDINT FINLINEA|
                 BOLEANO IDBOOL FINLINEA| 
                 REAL IDREAL FINLINEA DECVARIABLES |
                 CADENA IDCAD FINLINEA DECVARIABLES |
                 ENTERO IDINT FINLINEA DECVARIABLES |
                 BOLEANO IDBOOL FINLINEA DECVARIABLES ;

IDBOOL ::= IDENTIFICADOR | 
           IDENTIFICADOR COMA IDBOOL |
           IDENTIFICADOR SIGUAL OPBOLEANO | 
           IDENTIFICADOR SIGUAL OPBOLEANO COMA IDBOOL ;

IDREAL ::= IDENTIFICADOR |
           IDENTIFICADOR COMA IDREAL |
           IDENTIFICADOR SIGUAL DECIMAL |
           IDENTIFICADOR SIGUAL DECIMAL COMA IDREAL ;

IDCAD ::= IDENTIFICADOR |
          IDENTIFICADOR COMA IDCAD |
          IDENTIFICADOR SIGUAL CADTEXTO |
          IDENTIFICADOR SIGUAL CADTEXTO COMA IDCAD ;

IDINT ::= IDENTIFICADOR | 
          IDENTIFICADOR COMA IDINT | 
          IDENTIFICADOR SIGUAL NUMERO |  
          IDENTIFICADOR SIGUAL NUMERO COMA IDINT ;
/*------------------------------------------------------------------------------------------------------------------------------------------------*/            

